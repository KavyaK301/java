import java.util.Scanner;
public class Main {
    public static void main(String args[]) {
        
        Scanner n = new Scanner(System.in);
        int a =n.nextInt();
        
	     if(a > 0){
	         System.out.println(1);
	     }
	     else{
	         System.out.println(0);
	     }
		
    }
}
