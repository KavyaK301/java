mport java.util.*;

class Solution {
    public  static int computeXOR(int n){
        int ans=0;
        for(int i=1;i<=n;i++){
            ans=ans^i;
        }
        return ans;
      
    }
    
}

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(Solution.computeXOR(n));
    }
}
