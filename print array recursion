package p125;

public class printarrayrecursion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     int[] arr= {1,2,3,4,5};
     printarr(arr,0);
	}

	 public static void printarr(int[] arr,int i) {

		 if(i==arr.length) {

			 return;

		 }

		 System.out.print(arr[i]+" ");

		 printarr(arr,i+1);
     
     
	}

}
